{
    "formatterOptions": {
        "prefix": "app",
        "inputPath": "lib/localization/translations/remote",
        "outputPath": "lib/localization/translations/local",
        "translations": [
            {
                "locale": "ar",
                "input": "ar-Arabic.arb",
                "output": "ar.arb"
            },
            {
                "locale": "de",
                "input": "de-German.arb",
                "output": "de.arb"
            },
            {
                "locale": "en",
                "input": "en-English.arb",
                "output": "en.arb"
            }
        ],
        "keyPredicateMatch": {
            "-": "_",
            "_": "",
            ".": "",
            "^": "_",
            "(": "_",
            ")": "_"
        }
    },
    "generatorOptions": {
        "appLocalizations": "lib/localization/gen-l10n/app_localizations.dart",
        "translation": {
            "nullable": false,
            "message": "Cannot find translation-key!"
        }
    }
}